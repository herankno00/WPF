<UserControl x:Class="WpfApp2.UserControl1"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WpfApp2"
             mc:Ignorable="d" 
             Name="mainw"
            Width="480" HorizontalContentAlignment="Center" VerticalContentAlignment="Center" Height="21.333" Initialized="UserControl_Initialized">
    <StackPanel Width="400"   Orientation="Horizontal" >
        <ComboBox Style="{StaticResource ComboBoxStyle}"  SelectionChanged="Province_SelectionChanged" Name="Province"  VerticalAlignment="Top" Width="120"/>
        <ComboBox Style="{StaticResource ComboBoxStyle}"       SelectionChanged="Province_SelectionChanged_city"  Name="Citys" VerticalAlignment="Top" Width="120"/>
        <ComboBox Style="{StaticResource ComboBoxStyle}"   Name="county"  VerticalAlignment="Top" Width="120"/>
    </StackPanel>
</UserControl>
